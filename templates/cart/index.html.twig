                                {% if appointments is not empty %}
                                    {% for item in appointments %}
                                        <div class="appointment-item">
                                            {% if item.appointments[0] is defined %}
                                                <h2>Title: {{ item.appointments[0].title }}</h2>
                                                <p>ID: {{ item.appointments[0].id }}</p>
                                                <p>Heure de début du cours: {{ item.appointments[0].start|date("l d F Y à H:i") }}</p>
                                                <p>Heure de fin du cours: {{ item.appointments[0].end|date("l d F Y à H:i") }}</p>
                                                <p>Quantity: {{ item.quantity }}</p>
                                                <p>Tarif: {{ item.appointments[0].price / 100 }} Franc FCA</p>
                                                <p>Professeur: {{ item.appointments[0].teachers.firstname }} {{ item.appointments[0].teachers.lastname }}</p>
                                                <img src="/uploads/{{ item.appointments[0].teachers.illustration }}" alt="">
                                                <p><a href="{{ path('cart_remove', {'id' : item.appointments[0].id}) }}" class="btn btn-danger">
                                                <i class="fas fa-trash"></i>
                                                </a></p>
                                            {% else %}
                                                <p>Données manquantes pour cet élément</p>
                                            {% endif %}
                                        </div>
                                    {% endfor %}
                                   Prix total :  {{ total }} Franc CFA
                                {% else %}
                                    <p>Le panier est vide.</p>
                                {% endif %}





