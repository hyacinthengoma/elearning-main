    <script>
        // Récupérer les données du local storage
        let basketData = JSON.parse(localStorage.getItem('basket'));
        const panierDiv = document.getElementById('panier');
        const totalPriceElement = document.getElementById('total-price');

        // Fonction pour formater une date en français (jour, mois, année, heure sans secondes)
        function formatDateWithoutSeconds(dateString) {
            const date = new Date(dateString);
            const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric', hour: 'numeric', minute: 'numeric' };
            return date.toLocaleDateString('fr-FR', options);
        }

        // Fonction pour afficher les éléments du panier
        function displayBasket() {
            panierDiv.innerHTML = ''; // Effacer le contenu du panierDiv

            // Vérifier si le panier n'est pas vide
            if (!basketData || basketData.length === 0) {
                panierDiv.innerHTML = 'Le panier est vide.';
            } else {
                // Parcourir les données et afficher chaque événement
                for (let i = 0; i < basketData.length; i += 4) {
                    const selectedValue = basketData[i];
                    const start = formatDateWithoutSeconds(basketData[i + 1]);
                    const end = formatDateWithoutSeconds(basketData[i + 2]);
                    const price = (basketData[i + 3]);

                    // Créer des éléments HTML pour afficher les informations
                    const eventDiv = document.createElement('div');
                    eventDiv.innerHTML = `

                <div class="container custom-container-01">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="similler-course-list-wrap mt-0">
                                <ul class="ul simillar-course-list style-02 mt-0">
                                    <li class="single-simillar-course-item">
                                        <div class="thumb">

                                            <img src="/assets/img/Logos/logo4.png" alt="">
                                        </div>
                                        <div class="content">
                                            <div class="left-content">
                                                <h4 class="title">
                                                    <a href="#">Nom du cours:  ${selectedValue}</a>
                                                    <p href="#">Heure de début du cours: ${start}</p>
                                                    <p href="#">Heure de fin du cours : ${end}</p>


                                                </h4>
                                                <div class="rating-and-enrolled">
                                                    <div class="rating-wrap">
                                                        <span class="star">
                                                            <i class="fa-solid fa-star icon"></i>
                                                        </span>
                                                        <span class="nubm">4.8 (756)</span>
                                                    </div>
                                                    <div class="enrolled-wrap">
                                                        <i class="fa-solid fa-user-group icon"></i>
                                                        <span class="text">97538 Inscrits</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="right-content">
                                                <span class="remove" onclick="confirmRemoveItem(${i})">Supprimer</span>
                                                <span class="price">XOF ${price}</span>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            `;

                    // Ajouter l'élément au div du panier
                    panierDiv.appendChild(eventDiv);

                    // Planifier la suppression de l'événement du panier après 15 minutes
                    setTimeout(() => {
                        removeItem(i);
                    }, 15 * 60 * 1000); // 15 minutes en millisecondes
                }
            }
            // Calculer et afficher le prix total
            calculateTotalPrice();
        }

        // Fonction pour calculer le prix total des produits dans le panier
        function calculateTotalPrice() {
            let total = 0;

            for (let i = 0; i < basketData.length; i += 4) {
                const price = parseFloat(basketData[i + 3]); // Convertir le prix en nombre

                if (!isNaN(price)) {
                    total += price;
                }
            }

            // Afficher le prix total
            totalPriceElement.textContent = `XOF ${total}`;
        }


        // Fonction pour confirmer la suppression d'un élément du panier
        function confirmRemoveItem(index) {
            if (confirm("Voulez-vous vraiment supprimer cet élément du panier ?")) {
                removeItem(index);
            }
        }

        // Fonction pour supprimer un élément du panier
        function removeItem(index) {
            // Retirez les trois éléments associés du panierData
            basketData.splice(index, 3);

            // Mettez à jour le local storage avec les données mises à jour
            if (basketData.length === 0) {
                // Si le panier est maintenant vide, supprimez la clé "basket" du localStorage
                localStorage.removeItem('basket');
            } else {
                localStorage.setItem('basket', JSON.stringify(basketData));
            }

            // Appeler la fonction pour afficher le panier à jour
            displayBasket();
        }

        // Appeler la fonction initiale pour afficher le panier
        displayBasket();
    </script>











